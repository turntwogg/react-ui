{
  "include": ["src"],
  "exclude": ["node_modules"],
  "compilerOptions": {
    "declaration": true,
    "declarationDir": "./dist/",
    "strict": true,
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "module": "es6",
    "target": "es5",
    "jsx": "react",
    "esModuleInterop": true,
    "lib": ["dom", "es2017"]
  }
}
